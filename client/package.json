{
    "name": "client",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "start": "npm run dev",
        "build": "npm run lint && tsc && vite build",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write . --ignore-path .prettierignore",
        "format:check": "prettier --check . --ignore-path .prettierignore",
        "prepare": "husky install",
        "dev": "vite"
    },
    "dependencies": {
        "@clockworklabs/spacetimedb-sdk": "^1.3.1",
        "@gltf-transform/core": "^4.2.1",
        "@pixi/layout": "^3.1.0",
        "@pixi/ui": "^2.2.7",
        "@play-co/astro": "^9.9.10",
        "@play-co/commons": "^2.0.5",
        "@play-co/gb-model-tools": "^5.0.1",
        "@play-co/odie": "^13.3.1733950285",
        "pixi.js": "^8.8.1",
        "simple-links": "^2.0.0",
        "typed-signals": "^2.5.0"
    },
    "devDependencies": {
        "@assetpack/core": "^1.5.2",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.34.0",
        "@play-co/tsconfig": "^1.6.3",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.1.2",
        "eslint-plugin-prettier": "^5.2.6",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.5",
        "prettier": "^3.5.3",
        "typescript": "~5.7.3",
        "typescript-eslint": "^8.25.0",
        "vite": "^6.2.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write",
        "*.json": "prettier --write --ignore-path .prettierignore",
        "*.{js,jsx,ts,tsx}": "eslint --fix"
    }
}
